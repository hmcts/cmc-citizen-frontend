{% from "internalLink.njk" import internalLink %}
{% from "back-link/macro.njk" import govukBackLink %}
{% block top_of_page %}{% endblock %}
<!DOCTYPE html>
<!--[if lt IE 9]><html class="lte-ie8" lang="{{ html_lang|default('en') }}"><![endif]-->
<!--[if gt IE 8]><!--><html lang="{{ html_lang|default('en') }}"><!--<![endif]-->
<head>

<!-- Google Tag Manager -->
<script nonce='{{ nonce }}'>
  (function(w,d,s,l,i){w[l]=w[l]||[];w[l].push({'gtm.start':new Date().getTime(),event:'gtm.js'});var f=d.getElementsByTagName(s)[0],j=d.createElement(s),dl=l!='dataLayer'?'&l='+l:'';j.async=true;j.src='https://www.googletagmanager.com/gtm.js?id='+i+dl;f.parentNode.insertBefore(j,f);})(window,document,'script','dataLayer','{{ gtmScriptId }}');
</script>
<!-- End Google Tag Manager -->

  <meta charset="utf-8" />
  <title>{{ t(heading) }} - {{ t(serviceName) }}</title>

  <!--[if gt IE 8]><!--><link rel="stylesheet" media="screen" href="{{ asset_paths['style_vendor'] }}/govuk-template.css?{{ govuk_template_version }}"/><!--<![endif]-->
  <!--[if IE 6]><link rel="stylesheet" media="screen" href="{{ asset_paths['style_vendor'] }}/govuk-template-ie6.css?{{ govuk_template_version }}"/><![endif]-->
  <!--[if IE 7]><link rel="stylesheet" media="screen" href="{{ asset_paths['style_vendor'] }}/govuk-template-ie7.css?{{ govuk_template_version }}"/><![endif]-->
  <!--[if IE 8]><link rel="stylesheet" media="screen" href="{{ asset_paths['style_vendor'] }}/govuk-template-ie8.css?{{ govuk_template_version }}"/><![endif]-->
  <link rel="stylesheet" media="print" href="{{ asset_paths['style_vendor'] }}/govuk-template-print.css?{{ govuk_template_version }}"/>

  <link rel="stylesheet" media="all" href="{{ asset_paths['style_vendor'] }}/fonts.css?{{ govuk_template_version }}"/>
  <!--[if lt IE 9]><script src="{{ asset_paths['js_vendor'] }}/ie.js?{{ govuk_template_version }}"></script><![endif]-->

  <link rel="shortcut icon" href="{{ asset_paths['images_vendor'] }}/favicon.ico?{{ govuk_template_version }}" type="image/x-icon" />

  <link rel="mask-icon" href="{{ asset_paths['images_vendor'] }}/gov.uk_logotype_crown.svg?{{ govuk_template_version }}" color="#0b0c0c">
  <link rel="apple-touch-icon" sizes="180x180" href="{{ asset_paths['images_vendor'] }}/apple-touch-icon-180x180.png?{{ govuk_template_version }}">
  <link rel="apple-touch-icon" sizes="167x167" href="{{ asset_paths['images_vendor'] }}/apple-touch-icon-167x167.png?{{ govuk_template_version }}">
  <link rel="apple-touch-icon" sizes="152x152" href="{{ asset_paths['images_vendor'] }}/apple-touch-icon-152x152.png?{{ govuk_template_version }}">
  <link rel="apple-touch-icon" href="{{ asset_paths['images_vendor'] }}/apple-touch-icon.png?{{ govuk_template_version }}">

  <meta name="theme-color" content="#0b0c0c" />

  {% if development %}
    <link href="{{ asset_paths['style_vendor'] }}/HTMLCS.css" rel="stylesheet" />
  {% endif %}

  <meta name="viewport" content="width=device-width, initial-scale=1">

  {% block head %}{% endblock %}

  <meta property="og:image" content="{{ asset_paths['images_vendor'] }}/opengraph-image.png?{{ govuk_template_version }}">
</head>
<body class="{% block body_classes %}{% endblock %}">

<!-- Google Tag Manager (noscript) -->
<noscript>
  <iframe src="https://www.googletagmanager.com/ns.html?id={{ gtmScriptId }}&nojscript=true" height="0" width="0" style="display:none;visibility:hidden"></iframe>
</noscript>
<!-- End Google Tag Manager (noscript) -->

  <script>document.body.className = ((document.body.className) ? document.body.className + ' js-enabled' : 'js-enabled');</script>

  {% block body_start %}{% endblock %}

  <div id="skiplink-container">
    <div>
      {{ internalLink(skip_link_message|default('Skip to main content'), '#content', 'skiplink') }}
    </div>
  </div>
  <div>
  <link href="/stylesheets/govuk-frontend-3.11.0.min.css" media="screen" rel="stylesheet" type="text/css"/>
  {% include "../common/components/cookie-manager/cookie-banner.njk" %}
  </div>

  <header role="banner" id="global-header" class="{% block header_class %}{% endblock %}">
    <div class="header-wrapper">
      <div class="header-global">
        <div class="header-logo">
          <a href="{{ homepage_url|default('https://www.gov.uk') }}" title="{{ logo_link_title|default('Go to the GOV.UK homepage') }}" id="logo" class="content"
          style="background-image:none;">
          <span>
            <!--[if gt IE 8]><!-->
                  <svg
                    aria-hidden="true"
                    focusable="false"
                    class="govuk-header__logotype-crown"
                    xmlns="http://www.w3.org/2000/svg"
                    viewBox="0 0 32 30"
                    height="30"
                    width="32"
                  >
                    <path
                      fill="white" fill-rule="evenodd"
                      d="M22.6 10.4c-1 .4-2-.1-2.4-1-.4-.9.1-2 1-2.4.9-.4 2 .1 2.4 1s-.1 2-1 2.4m-5.9 6.7c-.9.4-2-.1-2.4-1-.4-.9.1-2 1-2.4.9-.4 2 .1 2.4 1s-.1 2-1 2.4m10.8-3.7c-1 .4-2-.1-2.4-1-.4-.9.1-2 1-2.4.9-.4 2 .1 2.4 1s0 2-1 2.4m3.3 4.8c-1 .4-2-.1-2.4-1-.4-.9.1-2 1-2.4.9-.4 2 .1 2.4 1s-.1 2-1 2.4M17 4.7l2.3 1.2V2.5l-2.3.7-.2-.2.9-3h-3.4l.9 3-.2.2c-.1.1-2.3-.7-2.3-.7v3.4L15 4.7c.1.1.1.2.2.2l-1.3 4c-.1.2-.1.4-.1.6 0 1.1.8 2 1.9 2.2h.7c1-.2 1.9-1.1 1.9-2.1 0-.2 0-.4-.1-.6l-1.3-4c-.1-.2 0-.2.1-.3m-7.6 5.7c.9.4 2-.1 2.4-1 .4-.9-.1-2-1-2.4-.9-.4-2 .1-2.4 1s0 2 1 2.4m-5 3c.9.4 2-.1 2.4-1 .4-.9-.1-2-1-2.4-.9-.4-2 .1-2.4 1s.1 2 1 2.4m-3.2 4.8c.9.4 2-.1 2.4-1 .4-.9-.1-2-1-2.4-.9-.4-2 .1-2.4 1s0 2 1 2.4m14.8 11c4.4 0 8.6.3 12.3.8 1.1-4.5 2.4-7 3.7-8.8l-2.5-.9c.2 1.3.3 1.9 0 2.7-.4-.4-.8-1.1-1.1-2.3l-1.2 4c.7-.5 1.3-.8 2-.9-1.1 2.5-2.6 3.1-3.5 3-1.1-.2-1.7-1.2-1.5-2.1.3-1.2 1.5-1.5 2.1-.1 1.1-2.3-.8-3-2-2.3 1.9-1.9 2.1-3.5.6-5.6-2.1 1.6-2.1 3.2-1.2 5.5-1.2-1.4-3.2-.6-2.5 1.6.9-1.4 2.1-.5 1.9.8-.2 1.1-1.7 2.1-3.5 1.9-2.7-.2-2.9-2.1-2.9-3.6.7-.1 1.9.5 2.9 1.9l.4-4.3c-1.1 1.1-2.1 1.4-3.2 1.4.4-1.2 2.1-3 2.1-3h-5.4s1.7 1.9 2.1 3c-1.1 0-2.1-.2-3.2-1.4l.4 4.3c1-1.4 2.2-2 2.9-1.9-.1 1.5-.2 3.4-2.9 3.6-1.9.2-3.4-.8-3.5-1.9-.2-1.3 1-2.2 1.9-.8.7-2.3-1.2-3-2.5-1.6.9-2.2.9-3.9-1.2-5.5-1.5 2-1.3 3.7.6 5.6-1.2-.7-3.1 0-2 2.3.6-1.4 1.8-1.1 2.1.1.2.9-.3 1.9-1.5 2.1-.9.2-2.4-.5-3.5-3 .6 0 1.2.3 2 .9l-1.2-4c-.3 1.1-.7 1.9-1.1 2.3-.3-.8-.2-1.4 0-2.7l-2.9.9C1.3 23 2.6 25.5 3.7 30c3.7-.5 7.9-.8 12.3-.8"></path>
                  </svg>
                  <!--<![endif]-->
                  <!--[if IE 8]>
                  <img src="{{ asset_paths['images_vendor'] }}/govuk-logotype-tudor-crown.png" class="govuk-header__logotype-crown-fallback-image" width="32" height="30" alt="">
                  <![endif]-->
             <span style="vertical-align: text-bottom;">{{ global_header_text|default('GOV.UK') }}</span>
             </span>
          </a>
        </div>
        {% block inside_header %}{% endblock %}
      </div>
      {% block proposition_header %}{% endblock %}
    </div>
  </header>

  {% block after_header %}{% endblock %}

  <div id="global-header-bar"></div>

  <main id="content" role="main">
    {% include "includes/phase_banner_beta.njk" %}
    {% block warning_message %}
      {% if warningBanner %}
        {% include "includes/banner.njk" %}
      {% endif %}
    {% endblock %}

    <div>
      {% block warning_message %}{% endblock %}
    </div>

    {% if form %}
      {% from "form.njk" import errorSummary %}
      {{ errorSummary(form) }}
    {% endif %}

    {% if govukBackLinkHref %}
      {{ govukBackLink({
        text: "Back",
        href: govukBackLinkHref
        })
      }}
    {% endif %}

    {% if heading and headingVisible %}
      <div class="grid-row">
        <div class="column-two-thirds">
          <h1 class="heading-{{ headingSize | default('large') }}">
            {{ t(heading) }}
            {% if headingSecondary %}
              <span class="heading-secondary">{{ t(headingSecondary) }}</span>
            {% endif %}
          </h1>
        </div>
      </div>
    {% endif %}

    {% block content %}{% endblock %}
    {% block contactForHelp %}
       {% include "includes/contact-us-for-help.njk" %}
        {% if antennawebChat %}
          <button class="chat-button">Talk to an Agent</button>
        {% endif %}
    {% endblock %}

  </main>

  <footer class="group js-footer" id="footer" role="contentinfo">

    <div class="footer-wrapper">
      {% block footer_top %}{% endblock %}

      <div class="footer-meta">
        <div class="footer-meta-inner">
          {% block footer_support_links %}{% endblock %}

          <div class="open-government-licence">
            <p class="logo"><a href="https://www.nationalarchives.gov.uk/doc/open-government-licence/version/3/" rel="license">Open Government Licence</a></p>

            {% block licence_message %}<p>{{ t('All content is available under the <a href="{{ link }}" rel="license">Open Government Licence v3.0</a>, except where otherwise stated', { link: 'https://www.nationalarchives.gov.uk/doc/open-government-licence/version/3/' }) | safe }}</p>{% endblock %}

          </div>
        </div>

        <div class="copyright">
          {{ internalLink(crown_copyright_message|default('&copy; ' + t('Crown copyright'))|safe, 'https://www.nationalarchives.gov.uk/information-management/re-using-public-sector-information/uk-government-licensing-framework/crown-copyright/') }}
        </div>
      </div>
    </div>
  </footer>

  <div id="global-app-error" class="app-error hidden"></div>

  <script src="{{ asset_paths['js_vendor'] }}/govuk-template.js?{{ govuk_template_version }}"></script>

  {% block body_end %}{% endblock %}

  <script>if (typeof window.GOVUK === 'undefined') document.body.className = document.body.className.replace('js-enabled', '');</script>
</body>
</html>
