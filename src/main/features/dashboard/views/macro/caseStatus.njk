{% macro caseStatusForClaimant(status, claim) %}
  {% if status === ClaimStatus.PAID_IN_FULL or status === ClaimStatus.PAID_IN_FULL_CCJ_CANCELLED or status === ClaimStatus.PAID_IN_FULL_CCJ_SATISFIED %}
    {{ paidInFullClaimantDashboard() }}
  {% elif status === ClaimStatus.NO_RESPONSE %}
    {{ noResponseForClaimantDashboard() }}
  {% elif status === ClaimStatus.MORE_TIME_REQUESTED %}
    {{ moreTimeRequestedForClaimantDashboard(claim) }}
  {% elif status === ClaimStatus.ELIGIBLE_FOR_CCJ %}
    {{ ccjEligibleForClaimantDashboard() }}
  {% elif status === ClaimStatus.CCJ_REQUESTED %}
    {{ ccjRequestedForClaimantDashboard(claim) }}
  {% elif status === ClaimStatus.REDETERMINATION_BY_JUDGE %}
    {{ redeterminationClaimantDashboard() }}
  {% elif status === ClaimStatus.CLAIMANT_ACCEPTED_ADMISSION_AND_REQUESTED_CCJ or status === ClaimStatus.CLAIMANT_ALTERNATIVE_PLAN_WITH_CCJ or
    status === ClaimStatus.CCJ_AFTER_SETTLEMENT_BREACHED or status === ClaimStatus.CCJ_BY_DETERMINATION_AFTER_SETTLEMENT_BREACHED %}
    {{ admissionByCCJClaimantDashboard(claim) }}
  {% elif status === ClaimStatus.ELIGIBLE_FOR_CCJ_AFTER_FULL_ADMIT_PAY_IMMEDIATELY_PAST_DEADLINE %}
    {{ claimFullAdmissionForClaimantDashboard(claim) }}
  {% elif status === ClaimStatus.CLAIMANT_ACCEPTED_ADMISSION_AND_DEFENDANT_NOT_SIGNED %}
    {{ settlementAgreementSignedByClaimantDeadlinePassedDashboard(claim) }}
  {% elif status === ClaimStatus.CLAIMANT_ACCEPTED_ADMISSION or status === ClaimStatus.CLAIMANT_ACCEPTED_COURT_PLAN_SETTLEMENT %}
    {{ settlementAgreementSignedByClaimantDashboard(claim) }}
  {% elif status === ClaimStatus.ADMISSION_SETTLEMENT_AGREEMENT_REACHED %}
    {{ settlementAgreementSignedByBothClaimantDashboard(claim) }}
  {% elif status === ClaimStatus.RESPONSE_SUBMITTED %}
    {% if claim.response.responseType === domain.ResponseType.FULL_DEFENCE %}
      {% if claim.response.defenceType === DefenceType.ALREADY_PAID %}
        {{ claimRejectedAlreadyPaidForClaimantDashboard(claim) }}
      {% elif claim.response.freeMediation === FreeMediationOption.NO %}
        {{ claimRejectedFreeMediationNOForClaimantDashboard(claim) }}
      {% else %}
        {{ claimRejectedFreeMediationYESForClaimantDashboard(claim) }}
      {% endif %}
    {% elif claim.response.responseType === domain.ResponseType.PART_ADMISSION %}
      {% if claim.response.paymentDeclaration and claim.totalAmountTillToday < claim.response.amount %}
        {{ claimPartAdmittedAlreadyPaidForClaimantDashboard(claim) }}
      {% else %}
        {{ claimPartAdmittedToOweForClaimantDashboard(claim) }}
      {% endif %}
    {% elif claim.response.responseType === domain.ResponseType.FULL_ADMISSION %}
      {{ claimFullAdmissionForClaimantDashboard(claim) }}
    {% endif %}
  {% elif status === ClaimStatus.OFFER_SUBMITTED %}
    {{ offerSubmittedForClaimantDashboard(claim) }}
  {% elif status === ClaimStatus.OFFER_REJECTED %}
    {{ offerRejectedForClaimantDashboard(claim) }}
  {% elif status === ClaimStatus.OFFER_ACCEPTED %}
    {{ offerAcceptedForClaimantDashboard(claim) }}
  {% elif status === ClaimStatus.OFFER_SETTLEMENT_REACHED
    or status === ClaimStatus.ELIGIBLE_FOR_CCJ_AFTER_BREACHED_SETTLEMENT %}
    {{ offerSettlementReachedDashboard() }}
  {% elif status === ClaimStatus.CLAIMANT_REJECTED_DEFENDANT_AS_COMPANY_OR_ORGANISATION_RESPONSE %}
    {{ claimantRejectedDefendantAsCompanyOrOrganisationResponse() }}
  {% elif status === ClaimStatus.CLAIMANT_REJECTS_PART_ADMISSION %}
    {{ partAdmitRejectedByClaimantDashboard(claim) }}
    {% elif status === ClaimStatus.CLAIMANT_ACCEPTED_DEFENDANT_FULL_ADMISSION_AS_BUSINESS_WITH_ALTERNATIVE_PAYMENT_INTENTION_RESPONSE %}
    {{ claimantWithDefendantAsBusinessResponse() }}
    {% elif status === ClaimStatus.CLAIMANT_ACCEPTED_DEFENDANT_PART_ADMISSION_AS_BUSINESS_WITH_ALTERNATIVE_PAYMENT_INTENTION_RESPONSE %}
    {{ claimantWithDefendantAsBusinessResponse() }}
  {% elif status === ClaimStatus.SETTLEMENT_AGREEMENT_REJECTED %}
    {{ settlementAgreementRejectedForClaimantDashboard(claim) }}
  {% elif status === ClaimStatus.PART_ADMIT_PAY_IMMEDIATELY %}
    {{ partAdmittedClaimantAcceptsClaimantDashboard(claim) }}
  {% endif %}
{% endmacro %}

{% macro caseStatusForDefendant(status, claim) %}
  {% if status === ClaimStatus.PAID_IN_FULL %}
    {{ paidInFullDefendantDashboard() }}
  {% elif status === ClaimStatus.PAID_IN_FULL_CCJ_CANCELLED or status === ClaimStatus.PAID_IN_FULL_CCJ_SATISFIED %}
    {{ paidInFullCancelledOrSatisfiedDefendantDashboard(claim) }}
  {% elif status === ClaimStatus.NO_RESPONSE %}
    {{ noResponseForDefendantDashboard(claim) }}
  {% elif status === ClaimStatus.MORE_TIME_REQUESTED %}
    {{ moreTimeRequestedForDefendantDashboard(claim) }}
  {% elif status === ClaimStatus.ELIGIBLE_FOR_CCJ %}
    {{ ccjEligibleForDefendantDashboard(claim) }}
  {% elif status === ClaimStatus.REDETERMINATION_BY_JUDGE %}
    {{ redeterminationDefendantDashboard(claim) }}
  {% elif status === ClaimStatus.CCJ_REQUESTED %}
    {{ ccjRequestedForDefendantDashboard(claim) }}
  {% elif status === ClaimStatus.CLAIMANT_ACCEPTED_ADMISSION_AND_REQUESTED_CCJ or status === ClaimStatus.CLAIMANT_ALTERNATIVE_PLAN_WITH_CCJ
    or status === ClaimStatus.CCJ_AFTER_SETTLEMENT_BREACHED or status === ClaimStatus.CCJ_BY_DETERMINATION_AFTER_SETTLEMENT_BREACHED %}
    {{ admissionByCCJDefendantDashboard(claim) }}
  {% elif status === ClaimStatus.CLAIMANT_ACCEPTED_ADMISSION or status === ClaimStatus.CLAIMANT_ACCEPTED_COURT_PLAN_SETTLEMENT
    or status === ClaimStatus.CLAIMANT_ACCEPTED_ADMISSION_AND_DEFENDANT_NOT_SIGNED %}
    {{ settlementAgreementSignedByClaimantDefendantDashboard(claim) }}
  {% elif status === ClaimStatus.ELIGIBLE_FOR_CCJ_AFTER_FULL_ADMIT_PAY_IMMEDIATELY_PAST_DEADLINE %}
    {{ claimFullAdmissionForDefendantDashboard(claim) }}
  {% elif status === ClaimStatus.ADMISSION_SETTLEMENT_AGREEMENT_REACHED %}
    {{ settlementAgreementSignedByBothDefendantDashboard(claim) }}
  {% elif status === ClaimStatus.RESPONSE_SUBMITTED %}
    {% if claim.response.responseType === domain.ResponseType.FULL_DEFENCE %}
      {% if claim.response.defenceType === DefenceType.ALREADY_PAID %}
        {{ claimRejectedAlreadyPaidForDefendantDashboard(claim) }}
      {% elif claim.response.freeMediation === FreeMediationOption.NO %}
        {{ claimRejectedFreeMediationNOForDefendantDashboard() }}
      {% else %}
        {{ claimRejectedFreeMediationYESForDefendantDashboard() }}
      {% endif %}
    {% elif claim.response.responseType === domain.ResponseType.PART_ADMISSION %}
      {% if claim.response.paymentDeclaration %}
        {{ claimPartAdmittedAlreadyPaidForDefendantDashboard(claim) }}
      {% else %}
        {{ claimPartAdmittedToOweForDefendantDashboard(claim) }}
      {% endif %}
    {% elif claim.response.responseType === domain.ResponseType.FULL_ADMISSION %}
      {{ claimFullAdmissionForDefendantDashboard(claim) }}
    {% endif %}
  {% elif status === ClaimStatus.OFFER_SUBMITTED %}
    {{ offerSubmittedForDefendantDashboard() }}
  {% elif status === ClaimStatus.OFFER_REJECTED %}
    {{ offerRejectedForDefendantDashboard() }}
  {% elif status === ClaimStatus.OFFER_ACCEPTED %}
    {{ offerAcceptedForDefendantDashboard() }}
  {% elif status === ClaimStatus.CLAIMANT_REJECTS_PART_ADMISSION %}
    {{ partAdmitRejectedByClaimantDefendantDashboard(claim) }}
  {% elif status === ClaimStatus.PART_ADMIT_PAY_IMMEDIATELY %}
    {{ partAdmittedClaimantAcceptsDefendantDashboard(claim) }}
  {% elif status === ClaimStatus.OFFER_SETTLEMENT_REACHED
    or status === ClaimStatus.ELIGIBLE_FOR_CCJ_AFTER_BREACHED_SETTLEMENT %}
  {{ offerSettlementReachedDashboard() }}
  {% elif status === ClaimStatus.CLAIMANT_ACCEPTED_DEFENDANT_FULL_ADMISSION_AS_BUSINESS_WITH_ALTERNATIVE_PAYMENT_INTENTION_RESPONSE %}
  {{  claimFullAdmissionForDefendantDashboard(claim) }}
  {% elif status === ClaimStatus.CLAIMANT_ACCEPTED_DEFENDANT_PART_ADMISSION_AS_BUSINESS_WITH_ALTERNATIVE_PAYMENT_INTENTION_RESPONSE %}
  {{ claimPartAdmittedToOweForDefendantDashboard(claim) }}
  {% elif status === ClaimStatus.SETTLEMENT_AGREEMENT_REJECTED %}
    {{ settlementAgreementRejectedForDefendantDashboard() }}
  {% endif %}
{% endmacro %}
