{% macro caseStatusForClaimant(status, claim) %}
  {% if status === ClaimStatus.PAID_IN_FULL or status === ClaimStatus.PAID_IN_FULL_CCJ_CANCELLED or status === ClaimStatus.PAID_IN_FULL_CCJ_SATISFIED
    or status === ClaimStatus.CLAIMANT_ACCEPTED_STATES_PAID %}
    {{ claimSettledDashboard() }}
 {% elif (status === ClaimStatus.NO_RESPONSE or status === ClaimStatus.ELIGIBLE_FOR_CCJ) and claim.helpWithFeesNumber and claim.state === 'HWF_APPLICATION_PENDING' %}
    {{ helpWithFeesValidationDashboard() }}
  {% elif (status === ClaimStatus.NO_RESPONSE or status === ClaimStatus.ELIGIBLE_FOR_CCJ) and claim.helpWithFeesNumber and claim.lastEventTriggeredForHwfCase === 'CreateHelpWithFeesClaim' %}
    {{ HWF_APPLICATION_PENDING() }}
  {% elif (status === ClaimStatus.NO_RESPONSE or status === ClaimStatus.ELIGIBLE_FOR_CCJ) and claim.helpWithFeesNumber and claim.lastEventTriggeredForHwfCase === 'InvalidHWFReference' %}
    {{ HWF_INVALID_REFERENCE() }}
  {% elif (status === ClaimStatus.NO_RESPONSE or status === ClaimStatus.ELIGIBLE_FOR_CCJ) and claim.helpWithFeesNumber and (claim.lastEventTriggeredForHwfCase === 'FullRemissionHWFRejected' or claim.lastEventTriggeredForHwfCase === 'NoRemissionHWF') %}
    {{ HWF_Rejected() }}
  {% elif (status === ClaimStatus.NO_RESPONSE or status === ClaimStatus.ELIGIBLE_FOR_CCJ) and claim.helpWithFeesNumber and claim.lastEventTriggeredForHwfCase === 'MoreInfoRequiredForHWF' %}
    {{ HWF_More_Info() }}
  {% elif (status === ClaimStatus.NO_RESPONSE or status === ClaimStatus.ELIGIBLE_FOR_CCJ) and claim.helpWithFeesNumber and claim.lastEventTriggeredForHwfCase === 'HWFPartRemission' %}
    {{ HWF_Part_Remitted() }}
  {% elif (status === ClaimStatus.NO_RESPONSE or status === ClaimStatus.ELIGIBLE_FOR_CCJ) and claim.helpWithFeesNumber and claim.lastEventTriggeredForHwfCase === 'HWFFullRemision' %}
    {{ HWF_Full_Granted() }}
  {% elif (status === ClaimStatus.NO_RESPONSE or status === ClaimStatus.ELIGIBLE_FOR_CCJ) and claim.helpWithFeesNumber and claim.lastEventTriggeredForHwfCase === 'RecalculateInterest' %}
    {{ HWF_Intrest_Recalculate() }}
  {% elif (status === ClaimStatus.NO_RESPONSE or status === ClaimStatus.ELIGIBLE_FOR_CCJ) and claim.helpWithFeesNumber and claim.lastEventTriggeredForHwfCase === 'UpdateHWFNumber' %}
    {{ HWF_Update_Number() }}
  {% elif status === ClaimStatus.NO_RESPONSE %}
    {{ noResponseForClaimantDashboard() }}
  {% elif status === ClaimStatus.MORE_TIME_REQUESTED %}
    {{ moreTimeRequestedForClaimantDashboard(claim) }}
  {% elif status === ClaimStatus.TRANSFERRED %}
    {{ claimTransferredClaimantClaimStatus() }}
  {% elif status === ClaimStatus.ELIGIBLE_FOR_CCJ and claim.state !== 'HWF_APPLICATION_PENDING' and claim.state !== 'AWAITING_RESPONSE_HWF' %}
    {{ ccjEligibleForClaimantDashboard() }}
  {% elif status === ClaimStatus.CCJ_REQUESTED %}
    {{ ccjRequestedForClaimantDashboard(claim) }}
  {% elif status === ClaimStatus.REDETERMINATION_BY_JUDGE %}
    {{ redeterminationClaimantDashboard() }}
  {% elif status === ClaimStatus.DEFENDANT_OCON_FORM_RESPONSE %}
    {{ claimRejectedWithDQsForClaimantDashboard(claim) }}
  {% elif status === ClaimStatus.DEFENDANT_PAPER_RESPONSE %}
    {{ paperResponseForClaimantDashboard() }}
  {% elif status === ClaimStatus.PROCEED_OFFLINE %}
    {{ offlineResponseForClaimantDashboard(claim) }}
  {% elif status === ClaimStatus.BUSINESS_QUEUE %}
    {{ businessQueueClaimantClaimStatus() }}
  {% elif status === ClaimStatus.CLAIMANT_ACCEPTED_ADMISSION_AND_REQUESTED_CCJ or
    status === ClaimStatus.CLAIMANT_ALTERNATIVE_PLAN_WITH_CCJ or
    status === ClaimStatus.CCJ_AFTER_SETTLEMENT_BREACHED or
    status === ClaimStatus.CCJ_BY_DETERMINATION_AFTER_SETTLEMENT_BREACHED or
    status === ClaimStatus.CLAIMANT_REQUESTS_CCJ_AFTER_DEFENDANT_REJECTS_SETTLEMENT %}
    {{ admissionByCCJClaimantDashboard(claim) }}
  {% elif status === ClaimStatus.ELIGIBLE_FOR_CCJ_AFTER_FULL_ADMIT_PAY_IMMEDIATELY_PAST_DEADLINE %}
    {{ claimFullAdmissionForClaimantDashboard(claim) }}
  {% elif status === ClaimStatus.ELIGIBLE_FOR_CCJ_AFTER_PART_ADMIT_PAY_IMMEDIATELY_PAST_DEADLINE %}
    {{ partAdmittedClaimantAcceptsClaimantDashboard(claim) }}
  {% elif status === ClaimStatus.CLAIMANT_ACCEPTED_ADMISSION_AND_DEFENDANT_NOT_SIGNED %}
    {{ settlementAgreementSignedByClaimantDeadlinePassedDashboard(claim) }}
  {% elif status === ClaimStatus.CLAIMANT_ACCEPTED_ADMISSION or status === ClaimStatus.CLAIMANT_ACCEPTED_COURT_PLAN_SETTLEMENT %}
    {{ settlementAgreementSignedByClaimantDashboard(claim) }}
  {% elif status === ClaimStatus.ADMISSION_SETTLEMENT_AGREEMENT_REACHED %}
    {{ settlementAgreementSignedByBothClaimantDashboard(claim) }}
  {% elif status === ClaimStatus.RESPONSE_SUBMITTED %}
    {% if claim.response.responseType === domain.ResponseType.FULL_DEFENCE %}
      {% if claim.response.defenceType === DefenceType.ALREADY_PAID %}
        {{ claimRejectedAlreadyPaidForClaimantDashboard(claim) }}
      {% elif claim.response.freeMediation === FreeMediationOption.NO %}
        {{ claimRejectedFreeMediationNOForClaimantDashboard(claim) }}
      {% else %}
        {{ claimRejectedFreeMediationYESForClaimantDashboard(claim) }}
      {% endif %}
    {% elif claim.response.responseType === domain.ResponseType.PART_ADMISSION %}
      {% if claim.response.paymentDeclaration and claim.totalAmountTillToday < claim.response.amount %}
        {{ claimPartAdmittedAlreadyPaidForClaimantDashboard(claim) }}
      {% else %}
        {{ claimPartAdmittedToOweForClaimantDashboard(claim) }}
      {% endif %}
    {% elif claim.response.responseType === domain.ResponseType.FULL_ADMISSION %}
      {{ claimFullAdmissionForClaimantDashboard(claim) }}
    {% endif %}
  {% elif status === ClaimStatus.CLAIMANT_REJECTED_DEFENDANT_DEFENCE %}
    {% if claim.claimantResponse.freeMediation === FreeMediationOption.NO %}
      {{ claimRejectedDefendantDefenceFreeMediationNOForClaimantDashboard(claim) }}
    {% else %}
      {% if (claim.mediationOutcome === MediationOutcome.FAILED) %}
        {{ mediationFailedForClaimantDashboard(claim) }}
      {% elif (claim.mediationOutcome === MediationOutcome.SUCCEEDED) %}
         {{ mediationSuccessForClaimantDashboard(claim) }}
      {% else %}
        {{ claimRejectedDefendantDefenceFreeMediationYESForClaimantDashboard(claim) }}
      {% endif %}
    {% endif %}
  {% elif status === ClaimStatus.CLAIMANT_ACCEPTED_DEFENDANT_DEFENCE %}
    {{ claimantAcceptedDefendantDefenceClaimantDashboard() }}
  {% elif status === ClaimStatus.OFFER_SUBMITTED %}
    {{ offerSubmittedForClaimantDashboard(claim) }}
  {% elif status === ClaimStatus.OFFER_REJECTED %}
    {{ offerRejectedForClaimantDashboard(claim) }}
  {% elif status === ClaimStatus.OFFER_ACCEPTED %}
    {{ offerAcceptedForClaimantDashboard(claim) }}
  {% elif status === ClaimStatus.OFFER_SETTLEMENT_REACHED
    or status === ClaimStatus.ELIGIBLE_FOR_CCJ_AFTER_BREACHED_SETTLEMENT %}
    {{ offerSettlementReachedDashboard() }}
  {% elif status === ClaimStatus.CLAIMANT_REJECTED_DEFENDANT_AS_BUSINESS_RESPONSE %}
    {{ partAdmitRejectedByClaimantDashboard(claim) }}
  {% elif status === ClaimStatus.CLAIMANT_REJECTED_PART_ADMISSION
    or status === ClaimStatus.CLAIMANT_REJECTED_PART_ADMISSION_DQ %}
    {{ partAdmitRejectedByClaimantDashboard(claim) }}
  {% elif status === ClaimStatus.CLAIMANT_REJECTED_STATES_PAID %}
    {{ statesPaidRejectedClaimantDashboard(claim) }}
  {% elif status === ClaimStatus.CLAIMANT_ACCEPTED_DEFENDANT_FULL_ADMISSION_AS_BUSINESS_WITH_ALTERNATIVE_PAYMENT_INTENTION_RESPONSE %}
    {{ claimantWithDefendantAsBusinessResponse() }}
  {% elif status === ClaimStatus.CLAIMANT_ACCEPTED_DEFENDANT_PART_ADMISSION_AS_BUSINESS_WITH_ALTERNATIVE_PAYMENT_INTENTION_RESPONSE %}
    {{ claimantWithDefendantAsBusinessResponse() }}
  {% elif status === ClaimStatus.SETTLEMENT_AGREEMENT_REJECTED %}
    {{ settlementAgreementRejectedForClaimantDashboard(claim) }}
  {% elif status === ClaimStatus.PART_ADMIT_PAY_IMMEDIATELY %}
    {{ partAdmittedClaimantAcceptsClaimantDashboard(claim) }}
  {% elif status === ClaimStatus.CLAIMANT_REJECTED_DEFENDANT_DEFENCE_NO_DQ %}
    {% if claim.claimantResponse.freeMediation === FreeMediationOption.NO %}
      {{ claimRejectedFreeMediationNOWithoutDQsForClaimantDashboard(claim) }}
    {% else %}
      {% if claim.mediationOutcome === MediationOutcome.FAILED %}
        {{ mediationFailedForClaimantDashboard(claim) }}
      {% elif (claim.mediationOutcome === MediationOutcome.SUCCEEDED) %}
        {{ mediationSuccessForClaimantDashboard(claim) }}
      {% else %}
        {{ claimRejectedFreeMediationYesWithoutDQsForClaimantDashboard(claim) }}
      {% endif %}
    {% endif %}
  {% elif status === ClaimStatus.DEFENDANT_REJECTS_WITH_DQS %}
    {{ claimRejectedWithDQsForClaimantDashboard(claim) }}
  {% elif status === ClaimStatus.ORDER_DRAWN %}
    {{ directionsOrderIssuedClaimantDashboard() }}
  {% elif status === ClaimStatus.BESPOKE_ORDER_DRAWN %}
    {{ directionsBespokeOrderIssuedClaimantDashboard() }}
  {% elif status === ClaimStatus.REVIEW_ORDER_REQUESTED %}
    {{ directionsOrderReviewRequestedClaimantDashboard(claim) }}
  {% elif status === ClaimStatus.INTENTION_TO_PROCEED_DEADLINE_PASSED %}
    {{ intentionToProceedPastDeadlineForClaimantDashboard(claim) }}
  {% endif %}
{% endmacro %}

{% macro caseStatusForDefendant(status, claim) %}
  {% if status === ClaimStatus.PAID_IN_FULL or status === ClaimStatus.CLAIMANT_ACCEPTED_STATES_PAID %}
    {{ claimSettledDashboard() }}
  {% elif status === ClaimStatus.PAID_IN_FULL_CCJ_CANCELLED or status === ClaimStatus.PAID_IN_FULL_CCJ_SATISFIED %}
    {{ paidInFullCancelledOrSatisfiedDefendantDashboard(claim) }}
  {% elif status === ClaimStatus.NO_RESPONSE %}
    {{ noResponseForDefendantDashboard(claim) }}
  {% elif status === ClaimStatus.MORE_TIME_REQUESTED %}
    {{ moreTimeRequestedForDefendantDashboard(claim) }}
  {% elif status === ClaimStatus.TRANSFERRED %}
    {{ claimTransferredDefendantClaimStatus() }}
  {% elif status === ClaimStatus.ELIGIBLE_FOR_CCJ %}
    {{ ccjEligibleForDefendantDashboard(claim) }}
  {% elif status === ClaimStatus.REDETERMINATION_BY_JUDGE %}
    {{ redeterminationDefendantDashboard(claim) }}
  {% elif status === ClaimStatus.CCJ_REQUESTED %}
    {{ ccjRequestedForDefendantDashboard(claim) }}
  {% elif status === ClaimStatus.DEFENDANT_OCON_FORM_RESPONSE%}
    {{ claimRejectedWithDQsForDefendantDashboard(claim) }}
  {% elif status === ClaimStatus.DEFENDANT_PAPER_RESPONSE %}
    {{ paperResponseForDefendantDashboard() }}
  {% elif status === ClaimStatus.PROCEED_OFFLINE %}
    {{ offlineResponseForDefendantDashboard(claim) }}
  {% elif status === ClaimStatus.BUSINESS_QUEUE %}
    {{ businessQueueDefendantClaimStatus() }}
  {% elif status === ClaimStatus.CLAIMANT_ACCEPTED_ADMISSION_AND_REQUESTED_CCJ or
    status === ClaimStatus.CLAIMANT_ALTERNATIVE_PLAN_WITH_CCJ or
    status === ClaimStatus.CCJ_AFTER_SETTLEMENT_BREACHED or
    status === ClaimStatus.CCJ_BY_DETERMINATION_AFTER_SETTLEMENT_BREACHED or
    status === ClaimStatus.CLAIMANT_REQUESTS_CCJ_AFTER_DEFENDANT_REJECTS_SETTLEMENT %}
    {{ admissionByCCJDefendantDashboard(claim) }}
  {% elif status === ClaimStatus.CLAIMANT_ACCEPTED_ADMISSION or status === ClaimStatus.CLAIMANT_ACCEPTED_COURT_PLAN_SETTLEMENT
    or status === ClaimStatus.CLAIMANT_ACCEPTED_ADMISSION_AND_DEFENDANT_NOT_SIGNED %}
    {{ settlementAgreementSignedByClaimantDefendantDashboard(claim) }}
  {% elif status === ClaimStatus.ELIGIBLE_FOR_CCJ_AFTER_FULL_ADMIT_PAY_IMMEDIATELY_PAST_DEADLINE %}
    {{ claimFullAdmissionForDefendantDashboard(claim) }}
  {% elif status === ClaimStatus.ELIGIBLE_FOR_CCJ_AFTER_PART_ADMIT_PAY_IMMEDIATELY_PAST_DEADLINE %}
    {{ partAdmittedClaimantAcceptsDefendantDashboard(claim) }}
  {% elif status === ClaimStatus.ADMISSION_SETTLEMENT_AGREEMENT_REACHED %}
    {{ settlementAgreementSignedByBothDefendantDashboard(claim) }}
  {% elif status === ClaimStatus.RESPONSE_SUBMITTED %}
    {% if claim.response.responseType === domain.ResponseType.FULL_DEFENCE %}
      {% if claim.response.defenceType === DefenceType.ALREADY_PAID %}
        {{ claimRejectedAlreadyPaidForDefendantDashboard(claim) }}
      {% elif claim.response.freeMediation === FreeMediationOption.NO %}
        {{ claimRejectedFreeMediationNOForDefendantDashboard() }}
      {% else %}
        {{ claimRejectedFreeMediationYESForDefendantDashboard() }}
      {% endif %}
    {% elif claim.response.responseType === domain.ResponseType.PART_ADMISSION %}
      {% if claim.response.paymentDeclaration %}
        {{ claimPartAdmittedAlreadyPaidForDefendantDashboard(claim) }}
      {% else %}
        {{ claimPartAdmittedToOweForDefendantDashboard(claim) }}
      {% endif %}
    {% elif claim.response.responseType === domain.ResponseType.FULL_ADMISSION %}
      {{ claimFullAdmissionForDefendantDashboard(claim) }}
    {% endif %}
  {% elif status === ClaimStatus.CLAIMANT_REJECTED_DEFENDANT_DEFENCE %}
    {% if claim.directionOrder !== undefined %}
      {{ directionsOrderIssuedDefendantDashboard() }}
    {% else %}
      {% if featureToggles.mediation and claim.claimantResponse.freeMediation === FreeMediationOption.YES %}
        {% if claim.mediationOutcome === MediationOutcome.FAILED %}
            {{ mediationFailedForDefendantDashboard(claim) }}
        {% elif claim.mediationOutcome === MediationOutcome.SUCCEEDED %}
          {{ mediationSuccessForDefendantDashboard(claim) }}
        {% else %}
            {{ claimRejectedDefendantDefenceFreeMediationYESForDefendantDashboard(claim) }}
        {% endif %}
      {% else %}
        {{ claimRejectedDefendantDefenceFreeMediationNOForDefendantDashboard(claim) }}
      {% endif %}
    {% endif %}
  {% elif status === ClaimStatus.CLAIMANT_ACCEPTED_DEFENDANT_DEFENCE %}
    {{ claimantAcceptedDefendantDefenceDefendantDashboard() }}
  {% elif status === ClaimStatus.OFFER_SUBMITTED %}
    {{ offerSubmittedForDefendantDashboard() }}
  {% elif status === ClaimStatus.OFFER_REJECTED %}
    {{ offerRejectedForDefendantDashboard() }}
  {% elif status === ClaimStatus.OFFER_ACCEPTED %}
    {{ offerAcceptedForDefendantDashboard() }}
  {% elif status === ClaimStatus.CLAIMANT_REJECTED_PART_ADMISSION
    or status === ClaimStatus.CLAIMANT_REJECTED_DEFENDANT_AS_BUSINESS_RESPONSE
    or status === ClaimStatus.CLAIMANT_REJECTED_PART_ADMISSION_DQ %}
    {{ partAdmitRejectedByClaimantDefendantDashboard(claim) }}
  {% elif status === ClaimStatus.CLAIMANT_REJECTED_STATES_PAID %}
    {{ statesPaidRejectedDefendantDashboard(claim) }}
  {% elif status === ClaimStatus.PART_ADMIT_PAY_IMMEDIATELY %}
    {{ partAdmittedClaimantAcceptsDefendantDashboard(claim) }}
  {% elif status === ClaimStatus.OFFER_SETTLEMENT_REACHED
    or status === ClaimStatus.ELIGIBLE_FOR_CCJ_AFTER_BREACHED_SETTLEMENT %}
    {{ offerSettlementReachedDashboard() }}
  {% elif status === ClaimStatus.CLAIMANT_ACCEPTED_DEFENDANT_FULL_ADMISSION_AS_BUSINESS_WITH_ALTERNATIVE_PAYMENT_INTENTION_RESPONSE %}
    {{  claimFullAdmissionForDefendantDashboard(claim) }}
  {% elif status === ClaimStatus.CLAIMANT_ACCEPTED_DEFENDANT_PART_ADMISSION_AS_BUSINESS_WITH_ALTERNATIVE_PAYMENT_INTENTION_RESPONSE %}
    {{ claimPartAdmittedToOweForDefendantDashboard(claim) }}
  {% elif status === ClaimStatus.SETTLEMENT_AGREEMENT_REJECTED %}
    {{ settlementAgreementRejectedForDefendantDashboard() }}
  {% elif status === ClaimStatus.SETTLEMENT_AGREEMENT_REJECTED %}
    {{ settlementAgreementRejectedForDefendantDashboard() }}
  {% elif status === ClaimStatus.CLAIMANT_REJECTED_DEFENDANT_DEFENCE_NO_DQ %}
    {% if claim.claimantResponse.freeMediation === FreeMediationOption.NO %}
      {{ claimRejectedFreeMediationNOWithoutDQsForDefendantDashboard(claim) }}
    {% else %}
      {% if claim.mediationOutcome === MediationOutcome.FAILED %}
        {{ mediationFailedForDefendantDashboard(claim) }}
      {% elif (claim.mediationOutcome === MediationOutcome.SUCCEEDED) %}
        {{ mediationSuccessForDefendantDashboard(claim) }}
      {% else %}
        {{ claimRejectedFreeMediationYesWithoutDQsForDefendantDashboard(claim) }}
      {% endif %}
    {% endif %}
  {% elif status === ClaimStatus.DEFENDANT_REJECTS_WITH_DQS %}
    {{ claimRejectedWithDQsForDefendantDashboard(claim) }}
  {% elif status === ClaimStatus.ORDER_DRAWN %}
    {{ directionsOrderIssuedDefendantDashboard() }}
  {% elif status === ClaimStatus.BESPOKE_ORDER_DRAWN %}
    {{ directionsBespokeOrderIssuedDefendantDashboard() }}
  {% elif status === ClaimStatus.REVIEW_ORDER_REQUESTED %}
    {{ directionsOrderReviewRequestedDefendantDashboard(claim) }}
  {% elif status === ClaimStatus.INTENTION_TO_PROCEED_DEADLINE_PASSED %}
    {{ intentionToProceedPastDeadlineForDefendantDashboard(claim) }}
  {% endif %}
{% endmacro %}
