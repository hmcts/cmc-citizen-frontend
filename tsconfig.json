{
  "compilerOptions": {
    "alwaysStrict": true,
    "noUnusedLocals": true,
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "lib": [
      "es6",
      "es2016.array.include",
      "es2017.object",
      "dom"
    ],
    "target": "es6",
    "module": "commonjs",
    "baseUrl": "src/main",
    "paths": {
      "app/*": [
        "app/*"
      ],
      "client/*": [
        "app/client/*"
      ],
      "postcode-lookup/*": [
        "app/postcode-lookup/*"
      ],
      "drafts/*": [
        "app/drafts/*"
      ],
      "claims/*": [
        "app/claims/*"
      ],
      "fees/*": [
        "app/fees/*"
      ],
      "forms/*": [
        "app/forms/*"
      ],
      "guards/*": [
        "app/guards/*"
      ],
      "health/*": [
        "app/health/*"
      ],
      "idam/*": [
        "app/idam/*"
      ],
      "logging/*": [
        "app/logging/*"
      ],
      "models/*": [
        "app/models/*"
      ],
      "utils/*": [
        "app/utils/*"
      ],
      "modules/*": [
        "modules/*"
      ],
      "routes/*": [
        "routes/*"
      ],
      "common/*": [
        "common/*"
      ],
      "claim/*": [
        "features/claim/*"
      ],
      "first-contact/*": [
        "features/first-contact/*"
      ],
      "response/*": [
        "features/response/*"
      ],
      "offer/*": [
        "features/offer/*"
      ],
      "statement-of-means/*": [
        "features/statement-of-means/*"
      ],
      "dashboard/*": [
        "features/dashboard/*"
      ],
      "ccj/*": [
        "features/ccj/*"
      ],
      "testing-support/*": [
        "features/testing-support/*"
      ],
      "services/*": [
        "services/*"
      ],
      "integration-tests/*": [
        "../../integration-tests/src/*"
      ]
    },
    "typeRoots": [
      "./node_modules/@types",
      "./types",
      "./integration-tests/types"
    ]
  }
}
